
<div class="card-container">
  <div class="form-card">
    <div class="title">
      <h1>Booking <%= @profile_sitter.user.first_name %> </h1>
    </div>
    <%= simple_form_for([@profile_sitter, @booking]) do |f| %>
      <%= f.input :start_date, as: :date, html5: true %>
      <%= f.input :end_date, as: :date, html5: true %>
      <%= f.submit class:'btn-booking'%>
    <% end %>
  </div>
</div>
<%# limiter les dates a celles qui correspondent au profil sitter %>
