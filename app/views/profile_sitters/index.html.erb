
<div class="container d-flex">
  <div class="cards mt-5">
    <% @profile_sitters.each do |profile_sitter| %>
    <div class="card-result-sitter" style="width: 18rem;">
      <img src="https://source.unsplash.com/user/allaboutanimals" class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-result-sitter-title"><%= profile_sitter.user.first_name %> <%= profile_sitter.user.last_name %></h5>
        <p class="card-result- sitter-text"> <%= profile_sitter.description %></p>
        <a href="#" class="btn-sitter">See more</a>
      </div>
    </div>
    <% end  %>
  </div>

  <div class="container-map mt-5 mx-5">
      <h1>bientot une map</h1>
  </div>
</div>
<%= link_to "Créer une nouvelle disponibilité", new_profile_sitter_path %>
